{
  "name": "rehman-welfare",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "serverD": "env-cmd --verbose -e development -r ./config/.env.json nodemon ./bin/www",
    "serverDev": "env-cmd --verbose -e development -r ./config/.env.json nodemon ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn run serverDev\" \"yarn run client\"",
    "build": "cd client && yarn run build",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.3.5",
    "sequelize-auto": "^0.6.3",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0"
  }
}
